	<bean id="categorySecond" class="com.zhonghao.categorysecond.domain.CategorySecond" scope="prototype"/>

	<!-- ÅäÖÃ Dao Bean -->
	<bean id="userDao" class="com.zhonghao.user.dao.impl.UserDaoImpl">
